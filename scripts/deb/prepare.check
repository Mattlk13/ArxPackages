#!/bin/bash

. "${BASH_SOURCE[0]%/*}/../common"
. "${BASH_SOURCE[0]%/*}/common.check"

[ -e "$phony" ] || exit_true

get_output _output

pushd "$_output"
	
	for ubuntu_ver in "${ubuntu_versions[@]}" ; do
		
		echo "Checking $ubuntu_ver package"
		[ -f *~${ubuntu_ver}*.dsc ] || exit_true
		
	done
	
popd

exit_false
