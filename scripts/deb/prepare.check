#!/bin/bash

. "${BASH_SOURCE[0]%/*}/../common"
. "${BASH_SOURCE[0]%/*}/common.check"
. "${BASH_SOURCE[0]%/*}/common"

[ -e "$phony" ] || exit_true

get_output _output

pushd "$_output"
	
	for ubuntu_ver in "${ubuntu_versions_src[@]}" ; do
		
		[ "$ubuntu_ver" = "$rebuild" ] && exit_true
		
		echo "Checking $ubuntu_ver package"
		[ -f *-0ppa1~${ubuntu_ver}*.dsc ] || exit_true
		
	done
	
popd

exit_false
